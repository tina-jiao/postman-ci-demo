					"script": {
						"exec": [
							"// 1. 解析接口返回的JSON响应数据",
							"var responseData = pm.response.json();",
							"",
							"// 2. 提取data节点下的id（核心：匹配你的JSON结构）",
							"var bookingId = responseData.data.id;",
							"",
							"// 3. 将id设置为全局变量（变量名自定义为global_bookingId，可任意命名）",
							"pm.globals.set(\"global_bookingId\", bookingId);",
							"",
							"// 可选：打印日志，验证提取的值（在Console中查看）",
							"console.log(\"提取的bookingId：\", bookingId);",
							"",
							"",
							"",
							"var jsonData = pm.response.json();  // 必须在所有测试之前定义",
							"",
							"pm.test(\"message为'OK'\", function () {",
							"    pm.expect(jsonData.message).to.eql(\"OK\");  // 现在可以访问了",
